<div class="page-header">
  <h1>
    Place types
    <a href="/place_types/new" class="btn btn-lg btn-success">New Place type</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Place type</th>
        <th>Actions</th>
      </tr>

      <% @place_types.each do |place_type| %>
      <tr>
        <td><%= place_type.place_type %></td>
        <td>
          <a href="/place_types/<%= place_type.id %>" class="btn btn-primary">Show</a>
          <a href="/place_types/<%= place_type.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_place_type/<%= place_type.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
